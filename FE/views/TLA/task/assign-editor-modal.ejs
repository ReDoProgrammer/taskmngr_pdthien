<div class="modal" tabindex="-1" id="assignEditorModal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-body text-dark">
        <div class="panel panel-info mb-3">
          <div class="panel-body">
            <div class="form-row">
              <div class="col-md-8 col-sm-6 col-xs-6">
                <select name="" id="slAssignEditor" class="form-control"></select>
              </div>
              <div class="col-md-2 col-sm-3 col-xs-3">
                <button class="btn btn-success" id="btnSubmitAssignEditor">Assign</button>
              </div>
              <div class="col-md-2 col-sm-3 col-xs-3">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">
                  Close
                </button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


    <script>
      $("#assignEditorModal").on('shown.bs.modal', function (e) {
        LoadEditor();
      })
      function LoadEditor() {
        $.ajax({
          url: '/tla/user/list-editor',
          type: 'get',
          headers: {
            authorization: "Bearer " + localStorage.getItem("accessToken"),
          },
          data: {
            levelId
          },
          success: function (data) {
            $('#slAssignEditor').empty();

            data.editors.forEach(e => {
              let opt = '<option value="' + e._id + '">' + e.fullname + '</option>';
              $('#slAssignEditor').append(opt);
            })
            $('#slAssignEditor').selectpicker();
            $('#slAssignEditor').selectpicker('refresh');

          },
          error: function (jqXHR, textStatus, errorThrown) {
            $('#msgError').text(jqXHR.responseJSON.msg);
          },
        })
      }
    </script>