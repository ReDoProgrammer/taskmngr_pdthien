<script>
	$('#btnAdminLogin').click(function () {
		let username = $('#txtUsername').val();
		let password = $('#txtPassword').val();
		$.ajax({
			url: '/tla/login',
			type: 'post',
			data: {
				username,
				password
			},
			success: function (data) {
				console.log(data);
			},
			error: function (request, status, error) {
				$('#lblError').text(request.responseJSON.msg);				
			}
		})
	})
</script>