<div class="modal" tabindex="-1" id="modalCustomerDetail">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Customer detail</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="form-row">
                    <div class="form-group col-md-2">
                        <label for="">Fullname:</label>
                    </div>
                    <div class="form-group col-md-4">
                        <label id="fullname" class="text-info font-weight-bold"></label>
                    </div>
                    <div class="form-group col-md-2 text-right">
                        <label>Phone:</label>
                    </div>
                    <div class="form-group col-md-4">
                        <label id="phone" class="text-info font-weight-bold"></label>
                    </div>
                </div>
                <div class="form-row">
                    <div class="col-md-2">
                        <label for="">Local storage:</label>
                    </div>
                    <div class="col-md-4">
                        <a href="http://" id="local_storage">Local storage</a>
                    </div>
                    <div class="col-md-2 text-right">
                        <label>Cloud storage:</label>
                    </div>
                    <div class="col-md-4">
                        <a href="http://" id="cloud_storage">Cloud storage</a>
                    </div>
                </div>

                <div class="form-row">
                    <div class="form-group col-md-2">
                        <label for="">Email:</label>
                    </div>
                    <div class="form-group col-md-4">
                        <label id="email" class="text-info font-weight-bold"></label>
                    </div>
                    <div class="form-group col-md-2 text-right">
                        <label>Address:</label>
                    </div>
                    <div class="form-group col-md-4">
                        <label id="address" class="text-info font-weight-bold"></label>
                    </div>
                </div>
                <hr>

                <div class="card bg-dark text-white mt-2">
                    <div class="card-header">Style list</div>
                    <div class="card-body">
                        <div class="form-row">
                            <label for="" class="text-info col-md-2">Output</label>
                            <label id="output" class="text-warning font-weight-bold col-md-2"></label>
                            <label for="" class="text-info col-md-2 text-right">Size</label>
                            <label id="size" class="text-warning font-weight-bold col-md-2"></label>
                            <label for="" class="text-info col-md-2 text-right">Color mode</label>
                            <label id="color_mode" class="text-warning font-weight-bold col-md-2"></label>
                        </div>
                        <div class="form-row">
                            <label for="" class="text-info col-md-2">Align</label>
                            <label id="align" class="text-warning font-weight-bold col-md-2"></label>
                            <label for="" class="text-info col-md-2 text-right">Cloud</label>
                            <label id="cloud" class="text-warning font-weight-bold col-md-2"></label>
                        </div>

                        <div class="form-row">
                            <label for="" class="text-info col-md-2">National style</label>
                            <label id="national_style" class="text-warning font-weight-bold col-md-2"></label>
                            <label for="" class="text-info col-md-2 text-right">Remark</label>
                            <label id="remark" class="text-warning font-weight-bold col-md-6"></label>
                        </div>
                        <hr class="text-danger">
                        <div class="card bg-dark text-white mt-2">
                            <div class="card-header">Special requests</div>
                            <div class="form-row">
                                <label for="" class="col-md-2 text-info text-right">TV</label>
                                <label id="tv" class="col-md-4 text-success">XX</label>
                                <label for="" class="col-md-2 text-info text-right">Grass</label>
                                <label id="grass" class="col-md-4 text-danger">XX</label>
                            </div>
                            <div class="form-row">
                                <label for="" class="col-md-2 text-info text-right">Sky</label>
                                <label id="sky" class="col-md-4 text-danger">XX</label>
                                <label for="" class="col-md-2 text-info text-right">Fire</label>
                                <label id="fire" class="col-md-4 text-danger">XX</label>
                            </div>
                            <div class="card-body">
                            </div>
                        </div>
                    </div>
                </div>



                <table class="table table-dark">
                    <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Level name</th>
                            <th scope="col" class="text-center">Price($)</th>
                        </tr>
                    </thead>
                    <tbody id="tblLevelList">
                    </tbody>
                </table>

            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>
</div>


